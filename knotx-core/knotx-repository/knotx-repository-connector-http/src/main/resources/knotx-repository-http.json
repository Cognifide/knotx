{
  "address": "knotx.core.repository.http",
  "client.options": {
    "maxPoolSize": 1000,
    "keepAlive": false,
    "tryUseCompression": true
  },
  "client.destination": {
    "domain": "localhost",
    "port": 3001
  },
  "allowed.request.headers": [
    "Accept*",
    "Authorization",
    "Connection",
    "Cookie",
    "Date",
    "Host",
    "If*",
    "Origin",
    "Pragma",
    "Proxy-Authorization",
    "User-Agent",
    "Via",
    "X-*"
  ]
}
