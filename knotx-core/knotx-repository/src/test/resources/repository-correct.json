{
  "config": {
    "repositories": [
      {
        "type": "local",
        "path": "/content/local/.*",
        "catalogue": ""
      },
      {
        "type": "remote",
        "path": "/content/.*",
        "domain": "localhost",
        "port": 8080,
        "client.options": {
          "tryUseCompression": true,
          "maxPoolSize": "1000",
          "keepAlive": false

        }
      }
    ]
  }
}