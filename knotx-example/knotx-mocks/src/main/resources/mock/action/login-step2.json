{
  "signals": {
    "success": {
      "data": {
        "result": "success"
      },
      "condition": {
        "password": "^knotx.*$"
      }
    },
    "error": {
      "data": {
        "validationError": "Invalid password address!"
      },
      "condition": {
        "password": "^(?!knotx).*$"
      }
    }
  }
}
